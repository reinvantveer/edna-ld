#!/usr/bin/nodejs

const bunyan = require('bunyan');

const logger = bunyan.createLogger({ name: 'edna etl app' });
const app = require('../app');

app.set('port', process.env.PORT || 3000);

const server = app.listen(app.get('port'), () => {
  logger.info(`Please point your browser to http://localhost:${server.address().port}`);
});
